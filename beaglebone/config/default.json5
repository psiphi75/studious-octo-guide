{
    /*
     * Define where the remote proxy will be.
     */
    "webRemoteControl": {
        "url": "localhost",   // The actual URL of the proxy
        "channel": "Honestly Round Down Quark",  // The channel we operate on
        "updateInterval": 50,    // How often we send a status update over the network (milliseconds)
        "useNetworkDiscovery": true     // Default to network discovery
    },

    /*
     * Define our location - required for figuring out true north.
     */
    "location": {
        "latitude": -36.4,
        "longitude": 174.2
    },

    /*
     * Which pins the servos are linked to.
     */
    "servos": {
        "sail": {
            "pin": "P8_13",
            "scalar": {
                "min": 0.03,
                "center": 0.09,
                "max": 0.20
            }
        },
        "rudder": {
            "pin": "P8_19",
            "scalar": {
                "min": -0.12,
                "center": 0.18,
                "max": 0.5
            }
        }
    },

    /*
     * Modem details
     */
    "modem": {
         "serialport": "/dev/ttyO1"
    },

    "gps": {
        "1": {
            "serialport": "/dev/ttyO2",
        },
        "2": {
            "serialport": "/dev/ttyO4",
        },
        "baudrate": 9600,
        "updateRate": 4             // Update rate in Hz
    },

    "mpu9250": {
        /*
        * How often we check the sensor samples (milliseconds).
        */
        "sampleInterval": 5,
        "sampleIntervalMagnetometer": 20,

        "options": {
            device: "/dev/i2c-2",

            // Enable/Disable magnetometer data (default false)
            UpMagneto: true,

            // If true, all values returned will be scaled to actual units (default false).
            // If false, the raw values from the device will be returned.
            scaleValues: true,

            // Enable/Disable debug mode (default false)
            DEBUG: true,

            // Set the Gyroscope sensitivity (default 0), where:
            //      0 => 250 degrees / second
            //      1 => 500 degrees / second
            //      2 => 1000 degrees / second
            //      3 => 2000 degrees / second
            GYRO_FS: 0,

            // Set the Accelerometer sensitivity (default 2), where:
            //      0 => +/- 2 g
            //      1 => +/- 4 g
            //      2 => +/- 8 g
            //      3 => +/- 16 g
            ACCEL_FS: 0,

            // These values were generated using calibrate_mag.js - you will want to create your own.
            magCalibration: {
                    min: {
                        x: -104.9921875,
                        y: -48.52734375,
                        z: -14.828125
                    },
                    max: {
                        x: 66.0625,
                        y: 121.91015625,
                        z: 164.25
                    },
                    offset: {
                        x: -19.46484375,
                        y: 36.69140625,
                        z: 74.7109375
                    },
                    scale: {
                        x: 0.657181,
                        y: 0.654810,
                        z: 0.688007
                    }
                },

            // These values were generated using calibrate_gyro.js - you will want to create your own.
            // NOTE: These are temperature dependent.
            gyroBiasOffset: {
                x: -0.478961832061068,
                y: -0.14835114503816793,
                z: 0.4523969465648852
            },

            // These values were generated using calibrate_accel.js - you will want to create your own.
            accelCalibration: { offset: {
                                   x: 0.011176961263020833,
                                   y: 0.003827718098958333,
                                   z: 0.03787556966145833
                               },
                               scale: {
                                   x: [-0.99033203125, 1.0129679361979167],
                                   y: [-0.9979768880208333, 1.0061800130208334],
                                   z: [-0.9573575846354166, 1.07052001953125]
                               }
            }


        }
    }
}
